{{page-title "Annotation"}}
<AuList @direction="vertical" as |Item|>
  <Item>{{@model.id}}</Item>
  <Item>{{@model.hasBody.label}}</Item>
  <Item>
    <AuLinkExternal href={{@model.hasBody.exactMatch}}>
      {{@model.hasBody.exactMatch}}
    </AuLinkExternal>
  </Item>
  <Item>Lambert/EPSG:31370: {{@model.hasBody.geometry.asWKT}}</Item>
  <Item>EPSG:4326/WGS84:
    {{#each this.geoPoints as |point|}}
      {{point.x}}
      {{point.y}}
    {{/each}}</Item>
</AuList>

{{map}}

{{outlet}}